<ul>
<%= if @current_user do %>
  <li><%= @current_user.email %></li>
  <li><%= link "Log out", to: Routes.pow_assent_authorization_path(@conn, :delete, :oauth2), method: :delete %></li>
<% else %>
  <li><%= link "Log in", to: Routes.pow_assent_authorization_path(@conn, :new, :oauth2) %></li>
<% end %>
</ul>
